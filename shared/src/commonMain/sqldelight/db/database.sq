CREATE TABLE diary (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  entry TEXT NOT NULL,
  date TEXT NOT NULL
);

selectAll:
SELECT * FROM diary ORDER BY id;

insert:
INSERT INTO diary(entry, date) VALUES (?, ?);

delete:
DELETE FROM diary WHERE id=?;

findByEntry:
SELECT * FROM diary WHERE entry LIKE ?;

findByDate:
SELECT * FROM diary WHERE date=?;

findByDateRange:
SELECT * FROM diary WHERE date BETWEEN ? AND ?;

deleteAll:
DELETE FROM diary;